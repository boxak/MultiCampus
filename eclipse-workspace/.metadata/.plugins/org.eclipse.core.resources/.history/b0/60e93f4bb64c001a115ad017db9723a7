package vo;

public class CalcVO {
	int num1;
	int num2;
	String operator;
	public int getNum1() {
		return num1;
	}
	public int getNum2() {
		return num2;
	}
	public String getOperator() {
		return operator;
	}
	public void setNum1(int num1) {
		this.num1 = num1;
	}
	public void setNum2(int num2) {
		this.num2 = num2;
	}
	public void setOperator(String operator) {
		this.operator = operator;
	}
	
}
